{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    // "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "rootDirs": ["./"],
  },
  "include": ["src/swagger-output.json","**/*.ts"],
  "exclude": ["node_modules"],
  "typeRoots": [
    "node_modules/@types"
  ],
  "types": [
    "node",
    "express"
  ]
}